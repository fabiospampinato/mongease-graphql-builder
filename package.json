{
  "name": "mongease-graphql-builder",
  "version": "1.0.6",
  "description": "Module for auto-generating simple GraphQL queries from Mongease descriptions.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/mongease-graphql-builder/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/mongease-graphql-builder.git"
  },
  "keywords": [
    "mongoose",
    "mongease",
    "graphql",
    "query",
    "generation"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.52",
    "graphql-tag": "^1.2.4",
    "lodash": "^4.17.4",
    "mongease": "^1.0.8",
    "mongease-graphql": "^1.0.7"
  },
  "devDependencies": {
    "rimraf": "^2.6.0",
    "typescript": "~3.0.3",
    "typescript-transform-export-interop": "^1.0.1"
  }
}
